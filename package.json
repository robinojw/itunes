{
  "name": "hp-itunes",
  "version": "1.0.0",
  "description": "HP Full Stack Test",
  "private": true,
  "author": "robinojw@gmail.com",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" --raw",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "dev:client": "vue-cli-service serve",
    "build:client": "npm run test && vue-cli-service build",
    "dev:server": "nodemon --config ./nodemon.config.json",
    "build:server": "rimraf dist/server && tsc -p tsconfig.server.json",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue"
    ],
    "preset": "ts-jest",
    "transform": {
      "^.+\\.ts?$": "ts-jest",
      ".*\\.(vue)$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|svg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "dependencies": {
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/test-utils": "^1.2.1",
    "axios": "^0.21.1",
    "babel-jest": "^27.0.2",
    "chalk": "^4.1.1",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "figlet": "^1.5.0",
    "jest": "^27.0.4",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "ts-jest": "^27.0.3",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-jest": "^3.0.7",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.5.1",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "^4.5.3",
    "@vue/cli-plugin-eslint": "^4.5.3",
    "@vue/cli-plugin-router": "^4.5.3",
    "@vue/cli-plugin-typescript": "^4.5.3",
    "@vue/cli-plugin-vuex": "~4.5.3",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-core": "^7.0.0-bridge.0",
    "concurrently": "^5.1.0",
    "eslint": "^7.7.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.0-beta.2",
    "jsdom-global": "^3.0.2",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^4.2.4",
    "vue-template-compiler": "^2.6.11"
  }
}
